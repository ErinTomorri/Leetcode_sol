class Solution:
    def maxArea(self, height: List[int]) -> int:
        area = 0
        left = 0
        right = len(height)-1

        while left<right:
            length = right - left
            height2 = min(height[left],height[right])
            area = max(area, length*height2)

            if height[left]<height[right]:
                left+=1
            else:
                right-=1
                
        return area
            
