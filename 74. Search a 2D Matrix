class Solution(object):
    def searchMatrix(self, matrix, target):
        for row in matrix:
            left = 0
            right = len(row) - 1
            
            while left <= right:
                mid = left + (right - left) // 2
                if row[mid] == target:
                    return True
                elif row[mid] < target:
                    left = mid + 1
                else:
                    right = mid - 1
        
        return False
